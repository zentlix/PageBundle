{
  "name": "zentlix/page-bundle",
  "type": "symfony-bundle",
  "description": "Zentlix Page bundle",
  "license": "proprietary",
  "require": {
    "php": "^7.4.4",
    "zentlix/main-bundle": "^1.0",
    "zentlix/route-bundle": "^1.0"
  },
  "require-dev": {
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Zentlix\\PageBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  }
}